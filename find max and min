def find_max_min_builtin(numbers):
    if not numbers:
        return None, None
    maximum = max(numbers)
    minimum = min(numbers)
    return maximum, minimum

# Example usage
numbers = [3, 1, 4, 1, 5, 9, 2, 6, 5, 3, 5]
max_value, min_value = find_max_min_builtin(numbers)
print(f"Maximum: {max_value}, Minimum: {min_value}")
